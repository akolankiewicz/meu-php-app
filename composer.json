{
  "name": "ixcsoft/meu-php-app",
  "description": "Projeto aula 2",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-http": "*",
    "ext-ctype": "*"
